# .env.example

# The full URL of the backend service you want to protect
TARGET_SERVICE_URL=http://mock-backend:5000

# Redis connection details
REDIS_HOST=redis
REDIS_PORT=6379

# Rate Limiting Configuration
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_MINUTE=100 # Maximum requests allowed per minute
RATE_LIMIT_WINDOW_SECONDS=60 # Time window in seconds for the rate limit